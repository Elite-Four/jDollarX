{
  "name": "jdollarx",
  "version": "0.0.1",
  "description": "Use JSX to build jQuery Objects.",
  "main": "jDollarX.js",
  "scripts": {
    "lint": "standard",
    "unit": "mocha",
    "dev": "mocha --watch",
    "precov": "babel test --out-dir lib-cov && echo --require should > lib-cov/mocha.opts",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- --opts lib-cov/mocha.opts lib-cov",
    "postcov": "rimraf lib-cov",
    "test": "npm run lint && npm run unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elite-Four/jDollarX.git"
  },
  "keywords": [
    "jsx",
    "jquery"
  ],
  "author": "George Chung <Gerhut@GMail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Elite-Four/jDollarX/issues"
  },
  "homepage": "https://github.com/Elite-Four/jDollarX#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-syntax-jsx": "^6.5.0",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-register": "^6.7.2",
    "istanbul": "^0.4.2",
    "jquery": "^3.0.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "should": "^8.3.0",
    "sinon": "^1.17.3",
    "standard": "^6.0.8"
  },
  "dependencies": {
    "jquery": "^3.0.0"
  }
}
